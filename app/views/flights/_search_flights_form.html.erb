<%= form_with url: "/flights", method: :get do |form|%>

    <%= form.label :From%>
    <%= select(:get, :from_id, @from_airport)%><br>

    <%= form.label :To%>
    <%= select(:get, :to_id, @to_airport)%><br>

    <%= form.label :Date%>
    <%= select(:get, :date, @date)%><br>


    <%= form.label :Number_of_Tickets%>
    <%= select(:get, :num_tickets, @num_tickets)%>

    <%=form.submit%>
<%end%>