<%= form_with model:@booking do |form|%>
    
    <br><br>
    
    <%= form.fields_for :passengers do |p_form| %>
        <%=p_form.label :Name%>
        <%=p_form.text_field :name%>

        <br>

        <%=p_form.label :Email%>
        <%=p_form.text_field :email%>
        <br><br>
    <%end%>

    <%=form.hidden_field :flight_id, value: flight.id %>

    <%=form.submit%>
<%end%>